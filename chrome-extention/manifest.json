{
  "name": "Cushy",
  "version": "2.0",
  "manifest_version": 2,
  "description": "cushy job!",
  "background": {
    "page": "background.html"
  },
  "icons": {
    "16" : "images/GB-19.png",
    "48" : "images/GB-48.png",
    "128" : "images/GB-128.png"
  },
  "browser_action": {
    "default_icon": "images/GB-19.png",
    "default_title": "Cushy"
  },
  "icons": {
    "16": "icon.png"
  },
  "content_scripts": [{
    "matches": ["http://*/*", "https://*/*"],
    "js": ["jquery-2.1.1.min.js", "storage_client.js", "save_dialog.js", "local_data.js", "form.js", "password_generator.js", "content_frame.js"],
    "all_frames": true
  }],
  "permissions": [
                   "webRequest",
                   "webRequestBlocking",
                   "*://*/*",
                   "storage",
                   "tabs",
                   "cookies"
                 ]
}
